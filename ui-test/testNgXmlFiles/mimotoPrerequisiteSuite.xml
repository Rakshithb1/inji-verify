<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

	<suite name="Mimoto Pre-requisite API Test">
	<test name="Adding Identity">
		<parameter name="ymlFile" value="injiverify/AddIdentity/AddIdentity.yml" />
		<parameter name="prerequisite" value="Yes" />
		<classes>
			<class name="io.mosip.testrig.apirig.injiverify.testscripts.AddIdentity" />
		</classes>
	</test>
	<test name="CreatePolicySunBirdR">
		<parameter name="ymlFile"
			value="injiverify/SunBirdR/CreatePolicySunBirdR/CreatePolicySunBirdR.yml" />
		<parameter name="idKeyName" value="osid" />
		<parameter name="prerequisite" value="Yes" />
		<classes>
			<class name="io.mosip.testrig.apirig.injiverify.testscripts.SimplePostForAutoGenId" />
		</classes>
	</test>
	<test name="GetPolicySunBirdR">
		<parameter name="ymlFile"
			value="injiverify/SunBirdR/GetPolicySunBirdR/GetPolicySunBirdR.yml" />
			<parameter name="prerequisite" value="Yes" />
		<classes>
			<class name="io.mosip.testrig.apirig.injiverify.testscripts.GetWithParam" />
		</classes>
	</test>
	<test name="SearchPolicySunBirdR">
		<parameter name="ymlFile"
			value="injiverify/SunBirdR/SearchPolicySunBirdR/SearchPolicySunBirdR.yml" />
			<parameter name="prerequisite" value="Yes" />
		<classes>
			<class name="io.mosip.testrig.apirig.injiverify.testscripts.SimplePost" />
		</classes>
	</test>
	
</suite>